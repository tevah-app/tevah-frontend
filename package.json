{
  "name": "tevahapp1",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build": "eas build",
    "configure": "eas build:configure",
    "a-local": "eas build --platform android --local",
    "i-local": "eas build --platform ios --local",
    "bpa": "eas build --profile preview --platform android",
    "bda": "eas build --profile development --platform android",
    "bpra": "eas build --profile production --platform android",
    "bpi": "eas build --profile preview --platform ios",
    "bpri": "eas build --profile production --platform ios",
    "bis": "eas build --profile development-simulator --platform ios",
    "submit-ios": "eas submit -p ios",
    "submit-android": "eas submit -p android",
    "build-local": "eas build --profile preview --platform android --local",
    "upgrade-eas": "npm install -g eas-cli",
    "upgrade-dependencies": "npx expo install --fix",
    "run-prod-bundle": "npx expo start --no-dev",
    "rebuild-dev-client": "npx expo prebuild --clean",
    "prebuild": "npx expo prebuild",
    "pod-install": "cd ios && pod install --repo-update",
    "expo-latest": "npx expo install expo@latest",
    "install-compatible": "npx expo install --check",
    "ota-prod": "eas update --branch production",
    "ota-prev": "eas update --branch preview",
    "ota-dev": "eas update --branch development --message 'Development update'"
  },
